{
  "name": "stock-chart",
  "version": "1.0.0",
  "description": "Displays the time from around the world",
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "webpack --progress --config webpack.config.dev.js",
    "build:prod": "webpack --progress --config webpack.config.prod.js",
    "lint": "tslint -p tsconfig.json -c tslint.yaml",
    "start": "npm run start:dev",
    "start:dev": "webpack-dev-server --progress --config webpack.config.dev.js",
    "test": "jest",
    "watch": "webpack --progress --colors --watch --config webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/reactivemarkets/Desktop/examples/stock-chart.git"
  },
  "keywords": [
    "example",
    "stocks",
    "chart"
  ],
  "author": "ReactiveMarkets",
  "license": "ISC",
  "dependencies": {
    "d3-dsv": "^1.2.0",
    "d3-format": "^1.4.3",
    "d3-time-format": "^2.2.3",
    "debug": "^4.1.1",
    "mobx": "^5.15.4",
    "mobx-react": "^6.1.8",
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-flexview": "^4.0.4",
    "react-stockcharts": "^0.7.8"
  },
  "devDependencies": {
    "@types/d3-dsv": "^1.0.36",
    "@types/d3-format": "^1.3.1",
    "@types/d3-time-format": "^2.1.1",
    "@types/debug": "4.1.5",
    "@types/jest": "^25.1.4",
    "@types/moment-timezone": "^0.5.13",
    "@types/react": "^16.9.28",
    "@types/react-dom": "^16.9.5",
    "acorn": "^7.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "^25.2.4",
    "mini-css-extract-plugin": "^0.9.0",
    "offline-plugin": "^5.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.13.1",
    "terser-webpack-plugin": "^2.3.5",
    "ts-jest": "^25.3.0",
    "ts-loader": "^6.2.2",
    "tslint": "^6.1.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
