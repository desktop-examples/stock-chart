{
  "name": "stock-chart",
  "version": "1.0.0",
  "description": "Displays the time from around the world",
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "webpack --progress --config webpack.config.dev.js",
    "build:prod": "webpack --progress --config webpack.config.prod.js",
    "lint": "tslint -p tsconfig.json -c tslint.yaml",
    "start": "webpack-dev-server --progress --config webpack.config.dev.js",
    "watch": "webpack --progress --colors --watch --config webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/desktop-examples/stock-chart.git"
  },
  "keywords": [
    "example",
    "stocks",
    "chart"
  ],
  "author": "Reactive Markets",
  "license": "Apache",
  "dependencies": {
    "d3-dsv": "^2.0.0",
    "d3-format": "^2.0.0",
    "d3-time-format": "^3.0.0",
    "debug": "^4.1.1",
    "mobx": "^5.15.6",
    "mobx-react": "^6.2.5",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-flexview": "^6.0.1",
    "react-stockcharts": "^0.7.8"
  },
  "devDependencies": {
    "@types/d3-dsv": "^1.0.36",
    "@types/d3-format": "^1.3.1",
    "@types/d3-time-format": "^2.1.1",
    "@types/debug": "4.1.5",
    "@types/moment-timezone": "^0.5.30",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "acorn": "^8.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.2.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "mini-css-extract-plugin": "^0.10.0",
    "offline-plugin": "^5.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "terser-webpack-plugin": "^4.1.0",
    "ts-loader": "^8.0.3",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.5.4",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
